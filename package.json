{
  "name": "pulsar",
  "version": "5.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/jadu/pulsar.git"
  },
  "scripts": {
    "pretest": "browserify tests/js/index --standalone=pulsar --debug --outfile dist/js/test.js",
    "test": "mocha-phantomjs tests/js/index.html --reporter spec",
