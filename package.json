    "grunt-real-favicon": "^0.1.3",
    "grunt-sass": "^1.1.0",
    "jquery": "1.x",
    "jsdom": "latest",
    "json2lcov": "^0.1.7",
    "matchdep": "^1.0.1",
    "minifyify": "^6.3.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "mocha-phantomjs": "^4.0.2",
    "phantomjs": "^1.9.19",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "uglifyify": "^3.0.1"
  },
  "browserify": {
    "transform": [
      "aliasify",
      "browserify-shim"
